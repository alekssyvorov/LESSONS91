from turtle import *


shape("turtle")
setup(500, 500)
title("New window")
pensize(4)
pencolor("#FF3399")
speed(1)


forward(100)
left(90)

forward(100)
left(90)

forward(100)
left(90)

forward(100)
left(90)
left(135)

up()
forward(300)

right(-90)

down()
pencolor('blue')
circle(100, 360, 4)


exitonclick()